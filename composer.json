{
    "name": "mediumart/notifier",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "notification", "channel"],
    "description": "Custom hook names for custom laravel notifications channels",
    "authors": [
        {
            "name": "Isaac Esso",
            "email": "daprod2009@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4",
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.9",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Mediumart\\Notifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Notifier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mediumart\\Notifier\\NotifierServiceProvider"
            ]
        }
    }
}
